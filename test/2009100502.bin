	 'type'	:	'iframe',
	 'titleShow'	:	false,
	 'centerOnScroll'	:	true,
	 'onComplete'	: function () {
	     $('#fancybox-overlay').height($(document).height());
	     $('#fancybox-content').css('overflow', 'auto').css('-webkit-overflow-scrolling', 'touch');
	 },
	 'onClosed' : function() {
	     location.reload();
	 }
     });
 });
